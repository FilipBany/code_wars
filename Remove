def remove_char(s):
    return s[1:-1]
# Przykłady użycia
print(remove_char("Hello"))  # Wynik: "ell"
print(remove_char("Python")) # Wynik: "ytho"
print(remove_char("abcd"))   # Wynik: "bc"
print(remove_char("A"))      # Wynik: "" (pusty string, bo usunięto jedyną literę)
print(remove_char(""))       # Wynik:
